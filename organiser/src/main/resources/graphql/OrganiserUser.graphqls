type OrganiserUser{
    id:ID!
    firstname:String!
    lastname:String!
    email:String!
    Password:String!
}

extend type Query{
    findUsers:[OrganiserUser]!
    findUserById(id:ID!):OrganiserUser!
    findUserByEmail(email:String!):OrganiserUser!
}

input OrganiserUserInput{
    firstname:String!,
    lastname:String!,
    email:String!,
    password:String!
}

extend type Mutation {
    createOrganiserUser(input:OrganiserUserInput): OrganiserUser!
}